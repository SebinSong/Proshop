{
  "name": "proshop",
  "version": "1.0.0",
  "description": "MERN stack practice",
  "proxy": "http://127.0.0.1:5000",
  "main": "index.js",
  "homepage": "https://sebinsong.github.io/Proshop/",
  "scripts": {
    "dev-frontend": "webpack serve --env mode=development --open",
    "dev-server": "nodemon backend/server",
    "dev": "concurrently \"npm:dev-server\" \"npm:dev-frontend\"",
    "build": "npm install && webpack --env mode=production",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "author": "Sebin Song",
  "license": "MIT",
  "nodemonConfig": {
    "ignore": [
      "./frontend/**"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "concurrently": "^8.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.16.1",
    "css-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.2.0",
    "mini-svg-data-uri": "^1.3.3",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.37.5",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.8.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@paypal/react-paypal-js": "^8.1.1",
    "@reduxjs/toolkit": "^2.0.0-alpha.5",
    "axios": "^1.3.5",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.2",
    "use-immer": "^0.9.0"
  }
}
